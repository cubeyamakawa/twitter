<%= form_for @user, url: settings_path, method: :put do |f| %>
  <div class="form-group">
	  <%= f.label :name, '名前', class: "control-label" %>
	  <%= f.text_field :name, class: "form-control" %>
  </div>
  <div class="form-group">
	  <%= f.label :displayname, 'ニックネーム', class: "control-label" %>
	  <%= f.password_field :displayname, class: "form-control" %>
  </div>
  <div class="form-group">
	  <%= f.label :prof, 'プロフィール', class: "control-label" %>
	  <%= f.text_area :prof, class: "form-control" %>
  </div>
  <div class="form-group">
	  <%= f.submit "更新する", class: "btn btn-primary" %>
  </div>
<% end %>