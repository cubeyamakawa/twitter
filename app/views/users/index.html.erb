<div class="col-xs-8#users-content">
  <div class="list-group">
    <div class="list-group-item user-list">
      <h2>ユーザー一覧</h2>
    </div>
    <% @users.each do |u|%>
      <div class="list-group-item">
        <h4 class="user">
          <span class="user-name">
            <%= link_to u.name, user_path(u)%>
          </span>
          <span>
            <%= "@"+u.name %>
          </span>
        </h4>
      </div>
    <% end %>
  </div>
</div>




