<p>
	<%= @user.id %>
	<%= @user.name %>
	<%= @user.prof %>
</p>


<% @user.tweets.each do |t| %>
	<div class="list-group-item">
	  <h4 class="user">
		<span class="user-name">
		  <%= link_to t.user.name, user_path(t.user) %>
		</span>
	  </h4>
	  <p>
	  	<%= t.content %>
	  </p>
	</div>
<% end %>
<p>
</p>
